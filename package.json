{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "HOST=0.0.0.0 rollup -c -w | sirv public --dev -s",
    "start": "sirv public --no-clear -s",
    "check": "svelte-check --fail-on-warnings --tsconfig ./tsconfig.json --ignore \"/public/build/\"",
    "format": "prettier --write .",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext svelte --ext js --max-warnings=0",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.1",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/gtag.js": "^0.0.8",
    "@types/three": "^0.136.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "draco3d": "^1.5.0",
    "eslint": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.0",
    "prettier-plugin-svelte": "^2.5.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.44.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-i18n": "^3.3.13",
    "svelte-preprocess": "^4.0.0",
    "svelte-toasts": "^1.1.2",
    "tslib": "^2.0.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@egjs/svelte-grid": "^1.6.0",
    "@sveltestack/svelte-query": "^1.5.0",
    "bulma": "^0.9.3",
    "dayjs": "^1.11.0",
    "gsap": "^3.9.0",
    "html-to-image": "^1.9.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.30",
    "qr-scanner": "^1.3.0",
    "query-string": "^6.13.8",
    "regenerator-runtime": "^0.13.9",
    "shortid": "^2.2.16",
    "sirv-cli": "^1.0.0",
    "socket.io-client": "^4.4.1",
    "svelte-carousel": "1.0.16",
    "svelte-collapsible": "^0.2.1",
    "svelte-forms-lib": "^2.0.0",
    "svelte-icons": "^2.1.0",
    "svelte-routing": "^1.6.0",
    "three": "^0.136.0",
